<!doctype html>
<html lang="es">
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Ziel Group es una empresa enfocada en ofrecer asesoría y gerencia financiera a pequeñas y medianas empresas.">
    <meta name="author" content="juanfrc - https://github.com/juanfrc">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Ziel Group" %></title>

    <%#= stylesheet_link_tag "animate", "default", "style" %>
    <%#= javascript_include_tag  "all" %>

    <link rel="shortcut icon" type="image/x-icon" href="img/Ziel_Group.ico"/>


    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap-theme.min.css">

    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">

    <link rel="stylesheet" href="stylesheets/animate.css">
    <link rel="stylesheet" href="stylesheets/default.css">
    <link rel="stylesheet" href="stylesheets/style.css">
     <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-38498500-4', 'auto');
      ga('send', 'pageview');

    </script>
  </head>

  <body id="page-top" data-spy="scroll" data-target=".navbar-custom">
    
    <%= yield %>

    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>




    <!-- Core JavaScript Files -->
    <script src="javascripts/jquery.easing.min.js"></script>    
    <script src="javascripts/jquery.scrollTo.js"></script>
    <script src="javascripts/wow.min.js"></script>
     <script src="javascripts/custom.js"></script>
     <script src="javascripts/all.js"></script>

  </body>
</html>
