              <form id="contact-form" action="https://getsimpleform.com/messages?form_api_token=6f3c790be0d3cbbf2d1abd39bdc16971" method="post">

              <div class="row">
                  <div class="col-md-6">
                      <div class="form-group">
                          <label for="name">
                              Nombre</label>
                          <input type='text' name='name' class="form-control" id="name" placeholder="Nombre" required="required"/>
                      </div>
                      <div class="form-group">
                          <label for="email">
                              Email</label>
                          <div class="input-group">
                              <span class="input-group-addon"><span class="glyphicon glyphicon-envelope"></span>
                              </span>
                              <input type="email" name="email" class="form-control" id="email" placeholder="nombre@nombre.com" required="required"/>
                          </div>
                      </div>
                      <div class="form-group">
                          <label for="subject">
                              Tema</label>
                          <select id="subject" name="subject" class="form-control" required="required">
                              <option value="no_subject" selected="">Escoja uno:</option>
                              <option value="Initial_contact">Contacto inicial</option>
                              <option value="Corporate_finance">Finanzas corporativas</option>
                              <option value="project_structure">Estructuración de proyectos</option>
                              <option value="family_office">Asesoría familiar</option>
                              <option value="personal_coach">Coach financiero</option>
                          </select>
                      </div>
                  </div>
                  <div class="col-md-6">
                      <div class="form-group">
                          <label for="name">
                              Mensaje</label>
                          <textarea name="message" id="message" class="form-control" rows="9" cols="25" required="required"
                              placeholder="Mensaje"></textarea>
                      </div>
                  </div>
                  <div class="col-md-12">
										  <!-- the redirect_to is optional, the form will redirect to the referrer on submission -->
										  <!-- <input type='hidden' name='redirect_to' value='http://www.naturaldelivery.co/sitio/?q=products' /> -->
                      <button type="submit" class="btn btn-skin pull-right" id="btnContactUs">
                          Enviar</button>
                  </div>
              </div>
              </form>
          </div>
